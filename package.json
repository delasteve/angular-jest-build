{
  "name": "angular-jest-build",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Angular Jest Build Facade",
  "main": "./src/builders/index.js",
  "typings": "./src/builders/index.d.ts",
  "builders": "./src/builders/builders.json",
  "schematics": "./src/schematics/collection.json",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "copy:files": "cpy {package.json,LICENSE,README.md} dist",
    "copy:json": "cpy {src/**/*.json,src/**/files/*,src/**/files/**/*} dist --parents",
    "postbuild": "yarn run copy:files && yarn run copy:json"
  },
  "dependencies": {
    "jest-editor-support": "^23.0.0"
  },
  "peerDependencies": {
    "@angular-devkit/architect": "^0.6.0",
    "@angular-devkit/core": "^0.6.0",
    "@angular-devkit/schematics": "^0.6.0",
    "jest": "^23.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.6.0",
    "@angular-devkit/core": "^0.6.0",
    "@angular-devkit/schematics": "^0.6.0",
    "@types/node": "^10.3.2",
    "cpy-cli": "^2.0.0",
    "jest": "^23.0.0",
    "jest-editor-support": "^23.0.0",
    "rxjs": "^6.0.0",
    "typescript": "~2.7.2"
  }
}
